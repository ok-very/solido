config_version=5

[application]

config/name="Solido Tri-D"
config/description="A stable, scalable graphics construction apparatus for GLSL-like shader development"
config/version="0.1.0"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
